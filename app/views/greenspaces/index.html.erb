
<% if @greenspaces.any? %>
  <% @greenspaces.each do |greenspace| %>
    <h2><%= link_to "Visit #{greenspace.name}", greenspace_path(greenspace) %></h2>
    <%= link_to "Edit #{greenspace.name}", edit_greenspace_path(greenspace) %>
    <br><br>
    <%= button_to "Delete #{greenspace.name}", greenspace_path(greenspace), method: :delete %>
    <br>

    <br><br>
--------------------------------------------------------
<br>
  <% end %>
<% else %>
  <h1> No greenspaces added yet</h1>
<% end %>
<p> <p>
<%= link_to('Add a greenspace', new_greenspace_path)%>
